  /*
   * ===========================================
   * WORKFLOWS SECTION
   * ===========================================
   * Business process automation definitions:
   * - Order processing workflows
   * - User onboarding sequences
   * - Notification triggers
   * - Service orchestration
   */
  "workflows": [
    {
      // Primary order workflow - handles all new orders
      "id": "wf_001",
      "name": "Order Processing" /* Most critical workflow */,
      "status": "active",
      "version": 5,
      "steps": [
        {
          "id": "step_001",
          "name": "Validate Order",
          "type": "validation",
          "config": {
            "rules": [
              { "field": "items", "check": "notEmpty" },
              { "field": "total", "check": "positive" },
              { "field": "customer.email", "check": "email" }
            ],
            "onFailure": "reject"
          },
          "next": "step_002",
          "timeout": null
        },
        {
          "id": "step_002",
          "name": "Check Inventory",
          "type": "service_call",
          "config": {
            "service": "inventory-service",
            "method": "checkAvailability",
            "retry": { "attempts": 3, "backoff": "exponential" }
          },
          "next": { "success": "step_003", "failure": "step_005" },
          "timeout": 30
        },
        {
          "id": "step_003",
          "name": "Process Payment",
          "type": "service_call",
          "config": {
            "service": "payment-service",
            "method": "charge",
            "retry": { "attempts": 2, "backoff": "linear" }
          },
          "next": { "success": "step_004", "failure": "step_006" },
          "timeout": 60
        },
        {
          "id": "step_004",
          "name": "Send Confirmation",
          "type": "notification",
          "config": {
            "channels": ["email", "push"],
            "template": "order_confirmation",
            "async": true
          },
          "next": null,
          "timeout": null
        },
        {
          "id": "step_005",
          "name": "Notify Out of Stock",
          "type": "notification",
          "config": {
            "channels": ["email"],
            "template": "out_of_stock",
            "async": true
          },
          "next": null,
          "timeout": null
        },
        {
          "id": "step_006",
          "name": "Payment Failed Handler",
          "type": "notification",
          "config": {
            "channels": ["email"],
            "template": "payment_failed",
            "async": true
          },
          "next": null,
          "timeout": null
        }
      ],
      "triggers": [
        { "type": "api", "endpoint": "/orders", "method": "POST" },
        { "type": "queue", "name": "orders.new" }
